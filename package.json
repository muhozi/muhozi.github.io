{
  "name": "muhozi.github.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.1",
    "ionicons": "^4.3.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && yarn build-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar ./src/styles/app.scss ./src/styles/app.css --include-path ./node_modules src/styles -o src/styles --output-style compressed",
    "watch-css": "node-sass-chokidar ./src/styles/app.scss ./src/styles/app.css --include-path ./node_modules src/styles -o src/styles --watch --recursive",
    "lint": "./node_modules/eslint/bin/eslint.js ./src",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "node-sass-chokidar": "^1.3.3"
  },
  "description": "This is muhozi profile page",
  "homepage": "https://muhozi.github.io",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:muhozi/muhozi.github.io.git"
  },
  "keywords": [
    "emery",
    "muhozi"
  ],
  "author": "Emery Muhozi",
  "license": "MIT"
}
